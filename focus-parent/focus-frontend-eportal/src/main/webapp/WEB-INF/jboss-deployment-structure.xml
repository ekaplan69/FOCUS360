<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
  <deployment>
	<dependencies>
	  <module name="org.jboss.ws.spi" export="true" />
	  <module name="org.apache.ws.security" export="true" />    <!-- necessary for ErvServlets -> SvWebErv call -->
	  <module name="org.apache.cxf" export="true" />
	  <module name="org.apache.cxf.impl" export="true" />
	  <module name="at.itsv.servicesecurity.soap" export="true" /> <!-- necessary for Service Security (ITSV) -->
	  <module name="at.itsv.berez.principal" />
	  <module name="at.itsv.berez.login.api" />
	  <module name="at.itsv.berez.login.impl" services="import" />
	  <module name="org.apache.httpcomponents.core" />
	</dependencies>
  </deployment>
</jboss-deployment-structure>
