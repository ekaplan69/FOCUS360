  <xs:element name="suchergebnis">
	<xs:annotation>
	  <xs:documentation>
		Das Suchergebnis besteht aus der Gesamtanzahl der gefundenen Dokumente und einer Liste der einzelnen Dokumente.
	  </xs:documentation>
	</xs:annotation>
	<xs:complexType>
	  <xs:sequence>
		<xs:element name="gesamtanzahl-dokumente" type="xs:long">
		  <xs:annotation>
			<xs:documentation>
			  Gibt die Gesamtanzahl der gefundenen Dokumente an. In der Liste der Dokumente sind möglicherweise weniger Dokumente enthalten.
			</xs:documentation>
		  </xs:annotation>
		</xs:element>
		<xs:element name="dokumente" type="dokumenteTyp">
		  <xs:annotation>
			<xs:documentation>
			  Liste der gefundenen Dokumente eingeschränkt auf die Anzahl pro Seite und für die angegebene Seite des Suchergebnisses (Paging).
			</xs:documentation>
		  </xs:annotation>
		</xs:element>
	  </xs:sequence>
	</xs:complexType>
  </xs:element>
  <xs:element name="gfSuchergebnis">
	<xs:annotation>
	  <xs:documentation>
